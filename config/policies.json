{
  "policies": [
    {
      "handler": {
        "export": "ReplaceStringOutboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "match": "null",
          "mode": "string",
          "replaceWith": "-1"
        }
      },
      "name": "replace-null-outbound",
      "policyType": "replace-string-outbound"
    },
    {
      "handler": {
        "export": "MonetizationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "meters": {
            "requests": 1
          }
        }
      },
      "name": "monetization-inbound",
      "policyType": "monetization-inbound"
    },
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false,
          "cacheTtlSeconds": 60
        }
      },
      "name": "api-key-inbound",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "ChangeMethodInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "method": "GET"
        }
      },
      "name": "change-method-inbound",
      "policyType": "change-method-inbound"
    },
    {
      "handler": {
        "export": "SetBodyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "body": ""
        }
      },
      "name": "set-body-inbound",
      "policyType": "set-body-inbound"
    },
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/transform-body-inbound)"
      },
      "name": "transform-body-inbound",
      "policyType": "custom-code-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "ip",
          "requestsAllowed": 2,
          "timeWindowMinutes": 1
        }
      },
      "name": "rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    }
  ]
}